<div>
   <div class="login card">
     <div class="card-content">
       <h1>Login</h1>
       <form action="/session" method="post">
         <label for="inputname">Username   </label>
         <input id="inputname" type="text" name="user[username]"/>

         <label for="inputpassword">Password</label>
         <input id="inputpassword" type="password" name="user[password]" />
       <br><br>
         <input class="loginsubmit submit" type="submit" class="btn btn-primary"/>
         <% if (flash.incorrectPassword) { %>
         <p class = "errormessage"><%=flash.incorrectPassword%></p>
         <%} %>
         <% if (flash.userDoesntExist) { %>
         <p class = "errormessage"><%=flash.userDoesntExist%></p>
         <%} %>
       </form>
    </div>
  </div>
  <div class="signup card">
    <div class="card-content">
      <h1>Sign Up</h1>
      <form action="/users/new" method="post">
        <label for="inputname2">Username (firstname.lastname)   </label>
        <input id="inputname2" type="text" name="user[username]"/>

        <label for="inputpassword2">Password</label>
        <input id="inputpassword2" type="password" name="user[password]" />
      <br><br>
        <input class="loginsubmit submit" type="submit" class="btn btn-primary"/>
        <% if (flash.duplicateName) { %>
        <p class = "errormessage"><%= flash.duplicateName %></p>
        <%} %>
      </form>
    </div>
 </div>
</div>
