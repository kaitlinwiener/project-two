<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="/style.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    <title></title>
  </head>
  <body>
    <h1>All Articles</h1>
      <% articles.forEach(function (article, i) { %>
      <div class = "article">
      <h3><%= article.title %></h3>
      <h4>By <%= article.author.username %></h4>
      <p>$<%= article.body %></p>
        <% }) %>
  </body>
</html>
